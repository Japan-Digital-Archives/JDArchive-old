Options +FollowSymlinks

<FilesMatch "^(internal|quicksearch|edit_tag|edit|curate).php$">
AuthName "Staff Access Only:"
AuthType Basic
AuthUserFile /usr/www/users/jedarchi/production/.htpasswd
Require valid-user
</FilesMatch>

RewriteEngine on

RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f

RewriteRule ^news.*$ /news/index.php [NC,L]
RewriteRule ^testimonial.*$ /mvc/index.php [NC,L,QSA]
RewriteRule ^([\w]+)(\/)*/?$ $1.php [NC,L]
