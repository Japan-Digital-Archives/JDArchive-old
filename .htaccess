Options +FollowSymlinks

<FilesMatch "^(quicksearch|edit_tag|edit|curate).php$">
AuthName "Staff Access Only:"
AuthType Basic
AuthUserFile /usr/www/users/jedarchi/production/.htpasswd
Require user arne
Require user jdstaff
Require user mlak-admin
</FilesMatch>

<FilesMatch "^internal.php$">
AuthName "Seed Staff Access Only:"
AuthType Basic
AuthGroupFile /usr/www/users/jedarchi/.htgrp
AuthUserFile /usr/www/users/jedarchi/production/.htpasswd
Require valid-user
</FilesMatch>

RewriteEngine on

RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f

RewriteRule ^news.*$ /news/index.php [NC,L]
RewriteRule ^testimonial.*$ /mvc/index.php [NC,L,QSA]
RewriteRule ^([\w]+)(\/)*/?$ $1.php [NC,L]
